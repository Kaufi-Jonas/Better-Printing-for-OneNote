             xmlns:sys="clr-namespace:System;assembly=System.Runtime"
             Startup="Application_Startup"
>
    <Application.Resources>
        <ResourceDictionary>
            <sys:String x:Key="LocalFolderTitle">Better-Printing</sys:String>
